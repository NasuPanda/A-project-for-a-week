{
  "Version": "2012-10-17",
  "Statement": [
      {
          "Effect": "Deny",
          "Principal": "*",
          "Action": "s3:*",
          "Resource": [
              "arn:aws:s3:::media-storage-for-anki-like-app",
              "arn:aws:s3:::media-storage-for-anki-like-app/*"
          ],
          "Condition": {
              "Bool": {
                  "aws:Public": "true"
              }
          }
      }
  ]
}
